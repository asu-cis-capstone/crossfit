<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="3.9dp"
        android:layout_weight="1">
        <Button
            android:text="|||"
            android:layout_width="49.1dp"
            android:layout_height="45.6dp"
            android:id="@+id/hamburgerButton" />
        <TextView
            android:text="Calculator"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/calcHeaderText"
            android:layout_marginLeft="90dp"
            android:layout_marginRight="85dp" />
        <Button
            android:text="Home"
            android:layout_width="wrap_content"
            android:layout_height="40.4dp"
            android:id="@+id/homeButton" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="0.0dp"
        android:layout_weight="1"
        android:layout_marginBottom="0.0dp">
        <EditText
            android:inputType="numberDecimal"
            android:layout_width="122.7dp"
            android:layout_height="wrap_content"
            android:id="@+id/oneRmCalc"
            android:layout_marginLeft="50dp" />
        <TextView
            android:text="= One Rep Max"
            android:textAppearance="?android:attr/textAppearanceLarge"
